{
    "AWSEBDockerrunVersion":2,
    "containerDefinations":[
      {
        "name": "client",
        "image": "sairahul3 / multi-client-10-14",
        "hostname": "client",
        "essential": false
      },
      {
        "name": "server",
        "image": "sairahul3 / multi-server-10-14",
        "hostname": "api",
        "essential": false
      },
      {
        "name": "worker",
        "image": "sairahul3 / multi-worker-10-14",
        "hostname": "worker",
        "essential": false
      },
      {
        "name": "nginx",
        "image": "sairahul3 / multi-nginx-10-14",
        "essential": true,
        "portMappings":[
            {
                "hostPort":80,
                "containerPort":80
            }
        ],
        "links":["client","server"]
      }
    ]
}